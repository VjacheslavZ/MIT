        section.course-designer
            .container
                .row
                    .col-sm-12
                        h2.course-designer__title Конструктор курса
                    .col-lg-8
                        p.course-designer__manual Выбирите тип курса и его составляющие, для того что бы записаться и обучаться тому, чего хотите именно вы.
            .container
                form
                    .row
                        .course-designer__form.clearfix#form-file
                            .col-lg-4
                                .course-designer__items
                                    p.course-designer__courses-name Название курса
                                    .course-designer__items-wrap
                                        span.course-designer__cours-name.course-designer__cours-name_selected Project Managment (Online)
                                            input(type="checkbox" value="Project Managment (Online)" id="managment-online")
                                            label(for="managment-online")
                                            i.fa.fa-long-arrow-right

                                        span.course-designer__cours-name Project Managment (Offline)
                                            input(type="checkbox" value="Project Managment (Offline)" id="managment-offline")
                                            label(for="managment-offline")
                                            i.fa.fa-long-arrow-right
                            .col-lg-4
                                .course-designer__items
                                    p.course-designer__courses-name Название блока
                                    .course-designer__items-wrap
                                        span.course-designer__cours-name.course-designer__cours-name_selected Полный курс
                                            input(type="checkbox" value="Project Managment (Online)" id="full-course")
                                            label(for="full-course")

                                        span.course-designer__cours-name Управление проектами
                                            input(type="checkbox" value="Project Managment (Offline)" id="project-management")
                                            label(for="project-management")

                                        span.course-designer__cours-name Управление командой
                                                input(type="checkbox" value="Project Managment (Offline)" id="team-management")
                                                label(for="team-management")

                                        span.course-designer__cours-name Работа с клиентами
                                            input(type="checkbox" value="Project Managment (Offline)" id="work-with-clients")
                                            label(for="work-with-clients")

                                        span.course-designer__cours-name Маркетинг
                                            input(type="checkbox" value="Project Managment (Offline)" id="marketing")
                                            label(for="marketing")

                                        span.course-designer__cours-name Продажи
                                            input(type="checkbox" value="Project Managment (Offline)" id="sales")
                                            label(for="sales")
                            .col-lg-4
                                .course-designer__items-chosed
                                    p.course-designer__courses-finish Вы выбрали курс:
                                    p.course-designer__courses-chosed IT Project Managment (Online)
                                    .course-designer__items-wrap
                                        ul
                                            li
                                                i.fa.fa-check
                                                span Управление проектами
                                            li
                                                i.fa.fa-check
                                                span Управление командой
                                            li
                                                i.fa.fa-check
                                                span Работа с клиентами

                    .row
                        .course-designer__form-contacts
                            .intermediate_item
                                .input_wrap
                                    i.fa.fa-check
                                    input(name='name', type='text', placeholder='ИМЯ ФАМИЛИЯ')
                            .intermediate_item
                                .input_wrap
                                    i.fa.fa-check
                                    input(name='phone', type='text', placeholder='Телефон').mask
                            .intermediate_item
                                .input_wrap
                                    i.fa.fa-check
                                    input(name='email', type='text', placeholder='E-mail')

                            input(type='hidden', name='form_name', value='baseForm')
                            input(type='hidden', name='form_type', value='normal')
                            .intermediate_item
                                input(type='submit', value='Записаться')
